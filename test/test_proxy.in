/* -*- c -*- */

header {
##include "lptree/lptree.h"
}

data { 
  LptTree *tree;
  LptProxy *proxy;
}

setup { 
  data->tree = lpt_tree_new();
  data->proxy = lpt_proxy_new(data->tree);
}

teardown {
  L_OBJECT_CLEAR(data->proxy);
  L_OBJECT_CLEAR(data->tree);
}

test { 
  PIF_CHECK(data->proxy);
}

test {
  lpt_proxy_create_share(data->proxy, "TESTSHARE", "/", 0);
}

test {
  lpt_proxy_connect_client(data->proxy, 1);
}
